---
import Servicios from "@/layouts/function-components/Servicios";

const {servicios} = Astro.props;
---

<section class="section features pb-0">
  <div class="container">
    <div class="row">
      <div class="mx-auto text-center lg:col-7">
        <h2 set:html="Nuestros servicios" />
      </div>
    </div>
    <div class="p mt-14 ">
      <div class="row">
        <Servicios servicios={servicios} />
      </div>
    </div>
  </div>
</section>
